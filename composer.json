{
  "name": "linkshare/oauth2-linkshare",
  "description": "OAuth provider for connecting to LinkShare's developer APIs.",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/trafficgate/oauth2-linkshare/issues",
    "source": "https://github.com/trafficgate/oauth2-linkshare"
  },
  "authors": [
    {
      "name": "Andrew Miller",
      "email": "andrew.miller@rakuten.com"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation"
  ],
  "require": {
    "php": ">=7.1.3",
    "league/oauth2-client": "~2.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2.0",
    "mockery/mockery": "~1.2",
    "phpunit/phpunit": "~8.1"
  },
  "autoload": {
    "psr-4": {
      "Linkshare\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Linkshare\\OAuth2\\Client\\Test\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
